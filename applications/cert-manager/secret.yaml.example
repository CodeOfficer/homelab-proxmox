# Template for Cloudflare API token secret
# Generated automatically by 'make deploy-k8s' from $CLOUDFLARE_API_TOKEN in .envrc
# DO NOT commit secret.yaml to git
---
apiVersion: v1
kind: Secret
metadata:
  name: cloudflare-api-token-secret
  namespace: cert-manager
type: Opaque
stringData:
  api-token: "${CLOUDFLARE_API_TOKEN}"
